comment "PCI devices"
       	depends on NET_RADIO && INET && PCI        
       	       
       	       	
config REALTEK_PCI_RTL8185
       	tristate "8185 802.11g chipset support (PCI)"
	depends on NET_RADIO && INET && PCI
	select REALTEK_IEEE80211
	---help---
	  Say Y if you have a PCI 802.11g device with Realtek RTL8185
	  chipset.
       	
config REALTEK_PCI_RTL8190
       	tristate "8190 802.11n chipset support (PCI)"
	depends on NET_RADIO && INET && PCI
	---help---
	  Say Y if you have a PCI 802.11n device with Realtek RTL8190
	  chipset.
	         	
config REALTEK_PCI_RTL8192SE
       	tristate "8192SE 802.11n chipset support (PCI-E)"
       	depends on NET_RADIO && INET && PCI        
       	---help---
         Say Y if you have PCI-E 802.11n devices with Realtek RTL8192SE
         chipset.         

config REALTEK_PCI_RTL8192CE
       	tristate "8192CE/8191CE/8188CE 802.11n chipset support (PCI-E)"
       	depends on NET_RADIO && INET && PCI
       	---help---
         Say Y if you have PCI-E 802.11n devices with Realtek 8192CE series
         chipset.         
